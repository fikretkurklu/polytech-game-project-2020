Mine(Banzai){
  * (Banzai) 
  | Cell(F,A) ? Explode  :(_)
  | True ? 80% Move(N) / Pop(S) / 5% Wizz / Jump : (Banzai)
  | Cell(N,V) & Cell(S,V) & Cell(E,V) & Cell(W,V) ? :(Banzai)
  | ! Key(FU) ? Move(N) :()
  * ()
}

Second(FS){
  * (FS) 
  | Cell(N,_) ? Explode  :(SS)
  | Cell (E,A) ? Move(S) :(SS)
  * (SS)
}
