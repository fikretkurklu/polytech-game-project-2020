Mine(Banzai){
  * (Banzai) 
  | Cell(F,A) ? Explode  :(_)
  | True ? 80% Move(N) / Pop(S) / 5% Wizz / Jump : (Banzai)
  | Cell(N,V) & Cell(S,V) & Cell(E,V) & Cell(O,V) ? :(Banzai)
  | !Key(FU) ? Move(N) :()
  * ()
}

Second(FS){
  * (FS) 
  | Cell(_,E) ? Explode  :(SS)
  | Cell (A,E) ? Move :(SS)
  * (SS)
  | True ? 80% Move / Pop / 5% Wizz / Jump : (FS)
}
