package playerCond;

import game.Entity;

public class CondBinaryOp implements ICondition {
	
	String operator;
	ICondition left;
	ICondition right;
	
	public CondBinaryOp(String operator, ICondition left, ICondition right) {
		this.operator = operator;
		this.left = left;
		this.right = right;
	}

	@Override
	public boolean eval(Entity e) {
		// TODO Auto-generated method stub
		return false;
	}

}
