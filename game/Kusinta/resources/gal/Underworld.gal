Cloud (Init){	
  * (Init)
  | Cell(H,O) ? Explode : ()
  | True ? Move : (Init)     
  * ()
}

PlayerSoul (Init){
  * (Visible)
  | Cell(H,O) ? Pop : (Actions)
  | !Cell(H,O) ? Wizz : (Actions)
  * (Actions)
  | Key(FU) ? Turn(N) : (Move)
  | Key(FD) ? Turn(S) : (Move)
  | Key(FL) ? Turn(L) : (Move)
  | Key(FR) ? Turn(R) : (Move)
  | Key(SPACE) ? Jump : (Visible)
  | Key(ENTER) ? Egg : (Visible)
  * (Move)
  | True ? Move : (Visible)
}