package automaton;

public class Transition {

	Condition m_condition;
	State initialState;
	State finalState;
	Action m_action;
	
	public Transition(Condition condition, State initialState, State finalState, Action action) {
		m_condition = condition;
		this.initialState = initialState;
		this.finalState = finalState;
		m_action = action;
	}
	
}
